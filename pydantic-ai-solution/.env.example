# German Energy Market Multi-Agent System Configuration
# Copy this file to .env and fill in your values

# Model Configuration
# Options: openai:gpt-4o-mini, openai:gpt-4o, openai:gpt-4-turbo, etc.
MODEL_NAME=openai:gpt-4o-mini

# API Keys (provide one of these)
OPENAI_API_KEY=your-openai-api-key-here
# GITHUB_TOKEN=your-github-token-here  # For GitHub Models

# Application Settings
DEBUG=false
LANGUAGE=de  # de for German, en for English
